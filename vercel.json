{
  "version": 2,
    "builds": [
        {
        "src": "dist/myserverside/server/server.mjs",
        "use": "@vercel/node",
        "config": {
            "includeFiles": [
            "dist/myserverside/server/**/*"
            ]
        }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "dist/myserverside/server/server.mjs"
        }   
    ]
 
}